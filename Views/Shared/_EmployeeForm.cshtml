@model HRMS.Models.Employee

<style>

</style>
<div class="container mt-4">
    <div class="row mb-3">
        <div class="col-md-3">
            <label asp-for="EmployeeCode" class="form-label">Employee Code:</label>
        </div>
        <div class="col-md-3">
            <input asp-for="EmployeeCode" class="form-control" />
        </div>
        <div class="col-md-3">
            <label asp-for="Name" class="form-label">Name:</label>
        </div>
        <div class="col-md-3">
            <input asp-for="Name" class="form-control" />
        </div>
    </div>

    <div class="row mb-3">
        <div class="col-md-3">
            <label asp-for="Email" class="form-label">Email:</label>
        </div>
        <div class="col-md-3">
            <input asp-for="Email" class="form-control" />
        </div>
        <div class="col-md-3">
            <label asp-for="Department" class="form-label">Department:</label>
        </div>
        <div class="col-md-3">
            <input asp-for="Department" class="form-control" />
        </div>
    </div>

    <div class="row mb-3">
        <div class="col-md-3">
            <label asp-for="Position" class="form-label">Position:</label>
        </div>
        <div class="col-md-3">
            <input asp-for="Position" class="form-control" />
        </div>
        <div class="col-md-3">
            <label asp-for="Salary" class="form-label">Salary:</label>
        </div>
        <div class="col-md-3">
            <input asp-for="Salary" type="number" class="form-control" />
        </div>
    </div>

    <div class="row mb-3">
        <div class="col-md-3">
            <label asp-for="JioTag" class="form-label">Jio Tag:</label>
        </div>
        <div class="col-md-3">
            <input asp-for="JioTag" class="form-control" />
        </div>
    </div>
</div>
